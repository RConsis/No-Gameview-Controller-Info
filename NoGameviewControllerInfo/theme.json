{
  "name": "No Gameview Controller Info",
  "version": "v1.0",
  "author": "RConsis",
  "manifest_version": 9,
  "id": "3af35d6-bs4g-sf3c-sg35-ksfhh0e434fgf",
  "patchControllerNavigation": true,
  "inject": {
    "shared.css": [
      "bigpicture"
    ]
  },
  "patches": {
    "Hide Badge": {
      "type": "checkbox",
      "values": {
        "No": {},
        "Yes": {
          "--hide-badge": [
            "none",
            "bigpicture"
          ]
        }
      }
    },
    "Hide Button": {
      "type": "checkbox",
      "values": {
        "No": {
          "--hide-button": [
            "flex",
            "bigpicture"
          ]
        },
        "Yes": {
          "--hide-button": [
            "none",
            "bigpicture"
          ]
        }
      }
    }
  }
}
